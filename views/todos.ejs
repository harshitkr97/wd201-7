<table border="5">
    <tr>
      <th>ID</th>
      <th>Title</th>
      <th>DueDate</th>
      <th>completed(True/False)</th>
    </tr>
    <% for(let i=0;i<allTodos.length; i++) { %>
      <tr>
        <td><%= allTodos[i].id %></td>
        <td><%= allTodos[i].title %></td>
        <td><%= allTodos[i].dueDate %></td>
        <td><%= allTodos[i].completed %></td>
      </tr>
    <% } %>
  </table>
